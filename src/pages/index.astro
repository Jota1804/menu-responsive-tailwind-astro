---
import Layout from '../layouts/Layout.astro';
import Principal from '../components/Principal.astro';
import About from '../components/About.astro';
import CTA from '../components/CTA.astro';
import Cards from '../components/Cards.astro';
import Last from '../components/Last.astro';
import Footer from '../components/Footer.astro';
import Principal2 from '../components/Principal2.astro';


---

<Layout title="Segundo profe">

  <div class="w-full h-screen">

    <section class="min-h-screen h-auto flex flex-col pt-28 justify-center items-center">
      <Principal2/>
    </section>

    <section class="bg-white py-16">
      <About/>
    </section>

    <section class="w-full h-screen flex items-center justify-center bg-slate-100">
      <Cards/>
    </section>

    <section class="px-4">
      <CTA/>
    </section>

    <section class="z-0 relative w-full h-screen flex justify-center items-center">
      <Last/>
    </section>

    <footer class="bg-gray-900 p-10 font-[sans-serif] tracking-wide">
      <Footer/>
    </footer>

  </div>
	  
</Layout>

